{
  "name": "MindustryKXI",
  "displayName": "MindustryKXI",
  "description": "MindustryKXI language syntax highlighting",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "kxi",
      "aliases": ["MKXI", "mkxi"],
      "extensions": [".mkxi"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "mkxi",
      "scopeName": "source.mkxi",
      "path": "./syntaxes/mkxi.tmLanguage.json"
    }]
  }
}
